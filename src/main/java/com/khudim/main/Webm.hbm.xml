<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.khudim.main.Webm" table="webm" schema="" catalog="yourwebm">
        <id name="id">
            <column name="id" sql-type="bigint" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="date">
            <column name="date" sql-type="date" not-null="true"/>
        </property>
        <property name="content">
            <column name="content" sql-type="longblob" not-null="true"/>
        </property>
        <property name="image">
            <column name="image" sql-type="longblob" not-null="true"/>
        </property>
        <property name="rating">
            <column name="rating" sql-type="int" not-null="true"/>
        </property>
        <property name="voteCount">
            <column name="vote_count" sql-type="bigint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>